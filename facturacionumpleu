class Cliente {
  id;
  nombre;
  direccion;
  telefono;
}

class Proveedor {
  id;
  nombre;
  direccion;
  telefono;
}

class Producto {
  id;
  nombre;
  precio;
  idProveedor;
}

class Articulo {
  id;
  cantidad;
  precio;
}

class Compra {
  id;
  total;
  formaPago;
  estado;
  fecha;
  Cliente cliente;
  Articulo articulo;
  Proveedor proveedor;
}

class Venta {
  id;
  total;
  formaPago;
  estado;
  fecha;
  Cliente cliente;
  Articulo articulo;
}
//$?[End_of_model]$?

